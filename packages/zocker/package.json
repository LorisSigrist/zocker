{
	"name": "zocker",
	"description": "Generate realistic test-data from your Zod-Schemas",
	"version": "2.2.1",
	"author": "Loris Sigrist",
	"license": "MIT",
	"keywords": [
		"mock-data",
		"zod"
	],
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/LorisSigrist/zocker.git"
	},
	"homepage": "https://zocker.sigrist.dev",
	"scripts": {
		"build": "tsdown --dts --format cjs --format esm",
		"test": "vitest --run --passWithNoTests",
		"check": "tsc --noEmit",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"tsdown": "^0.12.9",
		"typescript": "catalog:",
		"vitest": "catalog:",
		"zod": "^3.25.3"
	},
	"files": [
		"dist"
	],
	"peerDependencies": {
		"zod": "catalog:"
	},
	"dependencies": {
		"@faker-js/faker": "^9.8.0",
		"randexp": "^0.5.3"
	}
}
